{
    "name": "mixislv/reamaze-php-sdk",
    "type": "library",
    "description": "A PHP library for the Reamaze Core API",
    "license": "MIT",
    "keywords": [
        "reamaze",
        "api",
        "library",
        "sdk",
        "php"
    ],
    "authors": [
        {
            "name": "Mikus Rozenbergs",
            "homepage": "https://mixis.lv"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpmd/phpmd": "^2.7",
        "squizlabs/php_codesniffer": "^3.5",
        "php-coveralls/php-coveralls": "^2.2",
        "phpunit/php-code-coverage": "^7.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "mixisLv\\Reamaze\\": "src"
        }
    },
    "extra": {
       "component": "package",
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
