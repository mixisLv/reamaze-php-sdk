{
  "name": "mixislv/reamaze-php-sdk",
  "type": "library",
  "description": "A PHP library for the Reamaze Core API",
  "license": "MIT",
  "keywords": [
    "reamaze",
    "api",
    "library",
    "sdk",
    "php"
  ],
  "authors": [
    {
      "name": "Mikus Rozenbergs",
      "homepage": "http://mixis.lv"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.1",
    "phpunit/phpunit": "6.5.*",
    "phpunit/php-code-coverage": "^5.3",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "mixisLv\\Reamaze\\": "src"
    }
  },
  "extra": {
    "component": "package",
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
